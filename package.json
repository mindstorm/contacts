{
  "name": "contacts",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm-run-all prebuild:*",
    "prebuild:lint": "npm-run-all lint",
    "prebuild:test": "npm-run-all test",
    "build": "ng build",
    "postbuild": "npm-run-all postbuild:*",
    "postbuild:replace": "node ./tools/node/replace.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier -w \"src/**/*.{html,scss,ts}\"",
    "lint": "npm-run-all --parallel lint:*",
    "lint:html": "htmlhint \"src/**/*.html\" --nocolor",
    "lint:scss": "stylelint \"src/**/*.scss\" -f verbose --fix",
    "lint:ts": "ng lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "e2e": "ng e2e",
    "e2e:visual": "ng e2e --spec cypress/integration/visual.spec.ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:visual": "cypress run --spec cypress/integration/visual.spec.ts",
    "cypress:run:visual:percy": "percy exec -- cypress run --spec cypress/integration/visual.spec.ts",
    "cypress:run:visual:update": "cypress run --spec cypress/integration/visual.spec.ts --env updateSnapshots=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/cdk": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/forms": "13.2.2",
    "@angular/material": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/router": "13.2.2",
    "@ngrx/store": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "bootstrap": "5.1.3",
    "express": "4.17.2",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "uuid": "8.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "13.2.3",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.2.3",
    "@angular/compiler-cli": "13.2.2",
    "@angular/language-service": "13.2.2",
    "@cypress/schematic": "1.6.0",
    "@percy/cli": "1.0.0-beta.75",
    "@percy/cypress": "3.1.1",
    "@types/cypress-image-snapshot": "3.1.6",
    "@types/jest": "27.4.0",
    "@types/node": "12.20.15",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "chalk": "4.1.2",
    "conventional-changelog-cli": "2.2.2",
    "cypress": "9.4.1",
    "cypress-image-snapshot": "4.0.1",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "htmlhint": "1.1.2",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "prettier-plugin-css-order": "1.1.0",
    "replace-in-file": "6.3.2",
    "stylelint": "14.5.0",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.5.5",
    "yargs": "17.3.1"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.3.0"
  }
}